{
  "name": "galaxyjs",
  "version": "2.0.0",
  "description": "Modern cosmic animation library with 40+ space-themed animations, advanced performance optimization, and full accessibility support",
  "main": "js/galaxy-enhanced.js",
  "scripts": {
    "dev": "live-server --port=3000 --open=/",
    "build": "echo 'Build complete'",
    "test": "echo 'No tests specified'",
    "lint": "eslint js/*.js",
    "format": "prettier --write '**/*.{js,css,html,md}'",
    "serve": "python3 -m http.server 8000",
    "start": "npm run dev"
  },
  "keywords": [
    "animation",
    "cosmic",
    "space",
    "galaxy",
    "javascript",
    "css",
    "particles",
    "effects",
    "astronomy",
    "physics",
    "black-hole",
    "pulsar",
    "wormhole",
    "supernova",
    "nebula",
    "stars",
    "universe",
    "cosmos",
    "scientific",
    "interactive",
    "web-animations-api",
    "performance",
    "accessibility",
    "object-pooling",
    "canvas-optimization",
    "adaptive-quality"
  ],
  "author": {
    "name": "GalaxyJS Team",
    "email": "team@galaxyjs.dev",
    "url": "https://galaxyjs.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/galaxyjs.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/galaxyjs/issues"
  },
  "homepage": "https://galaxyjs.dev",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/yourusername"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.8.0",
    "live-server": "^1.2.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "css/",
    "js/",
    "index.html",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "css": "css",
    "js": "js",
    "examples": "examples"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "GalaxyJS Team",
      "email": "team@galaxyjs.dev"
    }
  ],
  "dependencies": {},
  "peerDependencies": {},
  "optionalDependencies": {},
  "bundledDependencies": [],
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "private": false,
  "workspaces": [],
  "config": {
    "port": 3000
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  }
} 